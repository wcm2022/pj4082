#else

    /* Older Visual Studio or non-Microsoft compiler */
    size_t n;

    /* Convert to multi-byte string (or count the number of bytes needed) */
    n = wcstombs (mbstr, wcstr, sizeInBytes);
    if (!mbstr  ||  n &lt; count) {

        /* Zero-terminate output buffer */
        if (mbstr  &amp;&amp;  sizeInBytes) {
            if (n &gt;= sizeInBytes) {
                n = sizeInBytes - 1;
            }
            mbstr[n] = '\0';
