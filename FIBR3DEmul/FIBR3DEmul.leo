    {
        auto &amp;registry_inst = details::registry::instance();

        // create global thread pool if not already exists..
        std::lock_guard&lt;std::recursive_mutex&gt; tp_lock(registry_inst.tp_mutex());
        auto tp = registry_inst.get_tp();
        if (tp == nullptr)
