    using internal::convert_arg;
    switch (*it++) {
    case 'h':
      if (*it == 'h')
        convert_arg&lt;signed char&gt;(arg, *++it);
      else
        convert_arg&lt;short&gt;(arg, *it);
      break;
    case 'l':
      if (*it == 'l')
        convert_arg&lt;long long&gt;(arg, *++it);
      else
