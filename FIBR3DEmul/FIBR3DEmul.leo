    }

    //origin to center
    //origin to center
    double oo[2];
    oo[0] = s[0] - center[0];
    oo[1] = s[1] - center[1];

    //radius
    m_radius = sqrt(pow(oo[0], 2) + pow(oo[1], 2));
    //start angle
    m_alpha = atan2(oo[1], oo[0]);

