<t tx="leo.20220407205634.68">void Path::SetCirclePath(double start[3], double center[3], Plane plane, bool clkwise)
{
    //copy to member variable
    copy(center, center + 3, m_center);

    double s[2];
    switch (plane) {
    case XY:
        s[0] = start[0];
