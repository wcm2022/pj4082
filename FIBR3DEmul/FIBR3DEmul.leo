std::vector&lt;double&gt; Trajectory::linspace(double a, double b, size_t N)
{
    vector&lt;double&gt; xs(N);
    vector&lt;double&gt;::iterator it;
    
    double h = (b - a) / N;
    double val;
    for (it = xs.begin(), val = a; it != xs.end(); ++it, val += h)
    {
        *it = val;
    }

    return xs;
}

